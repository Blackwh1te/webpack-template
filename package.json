{
  "name": "webpack-template",
  "version": "1.0.3",
  "description": "",
  "private": true,
  "scripts": {
    "start": "webpack s --mode development --config ./webpack.config.js",
    "build": "webpack --mode production --config ./webpack.config.js",
    "test": "jest --no-cache --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blackwh1te/webpack-template.git"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack"
  ],
  "author": "Aleksander Lamkov <aleksanderlamkov@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/Blackwh1te/webpack-template#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.4",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-loader": "^8.2.3",
    "baron": "^3.0.3",
    "beautify-html-webpack-plugin": "^1.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "ejs": "^3.1.6",
    "ejs-compiled-loader": "^3.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.9.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^7.0.0",
    "imports-loader": "^3.1.1",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^19.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "path": "^0.12.7",
    "postcss": "^8.4.6",
    "postcss-100vh-fix": "^1.0.2",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.18.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^12.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-extend-rule": "^4.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-for": "^2.1.1",
    "postcss-functions": "^4.0.2",
    "postcss-gradient-transparency-fix": "^4.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-mixins": "^9.0.1",
    "postcss-modules-extend-rule": "^0.1.0",
    "postcss-momentum-scrolling": "^3.14.22",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.0",
    "postcss-strip-inline-comments": "^0.1.5",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-webpack-plugin": "^3.1.1",
    "svg-sprite-loader": "6.0.11",
    "svgo": "^2.8.0",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.69.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "@fancyapps/ui": "^4.0.6",
    "baron": "^3.0.3",
    "imask": "^6.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reset-css": "^5.0.1",
    "scrollmagic": "^2.0.8",
    "svg4everybody": "2.1.9",
    "swiper": "^8.0.6",
    "tua-body-scroll-lock": "^1.2.1"
  },
  "browserslist": [
    "safari 14",
    "firefox 90",
    "edge 17",
    "chrome 90",
    "last 2 ios_saf versions",
    "last 2 and_chr versions",
    "last 2 samsung versions"
  ],
  "jest": {
    "testURL": "http://localhost",
    "automock": false,
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/js/test"
    ],
    "moduleNameMapper": {
      ".+\\.(css|pcss|png|mp3|mp4|jpeg|svg|jpg|gif|webp|ttf|woff|woff2)$": "jest-transform-stub",
      "swiper/css/bundle": "identity-obj-proxy"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ],
      [
        "@babel/plugin-transform-runtime"
      ]
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "node_modules/",
      "assets/",
      "modules/",
      "bitrix/"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "$": "readonly",
      "App": "writable",
      "ymaps": "readonly",
      "BX": "readonly",
      "grecaptcha": "readonly",
      "ScrollMagic": "readonly",
      "Fancybox": "readonly",
      "DocumentTouch": "readonly",
      "SITE_LANG": "readonly"
    },
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "function-call-argument-newline": [
        "error",
        "never"
      ],
      "block-spacing": "error",
      "arrow-spacing": "error",
      "constructor-super": 2,
      "for-direction": 2,
      "getter-return": 2,
      "no-async-promise-executor": 2,
      "no-case-declarations": 2,
      "no-class-assign": 2,
      "no-compare-neg-zero": 2,
      "no-cond-assign": 2,
      "no-const-assign": 2,
      "no-constant-condition": 2,
      "no-control-regex": 2,
      "no-debugger": 2,
      "no-delete-var": 2,
      "no-dupe-args": 2,
      "no-dupe-class-members": 2,
      "no-dupe-else-if": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-empty-character-class": 2,
      "no-empty-pattern": 2,
      "no-ex-assign": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-semi": 2,
      "no-fallthrough": 2,
      "no-func-assign": 2,
      "no-global-assign": 2,
      "no-import-assign": 2,
      "no-inner-declarations": 2,
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": 2,
      "no-misleading-character-class": 2,
      "no-mixed-spaces-and-tabs": 2,
      "no-new-symbol": 2,
      "no-obj-calls": 2,
      "no-octal": 2,
      "no-redeclare": 2,
      "no-regex-spaces": 2,
      "no-self-assign": 2,
      "no-setter-return": 2,
      "no-shadow-restricted-names": 2,
      "no-sparse-arrays": 2,
      "no-this-before-super": 2,
      "no-undef": 2,
      "no-unexpected-multiline": 2,
      "no-unreachable": 2,
      "no-unsafe-finally": 2,
      "no-unsafe-negation": 2,
      "no-unused-labels": 2,
      "no-with": 2,
      "require-yield": 2,
      "use-isnan": 2,
      "valid-typeof": 2,
      "no-restricted-exports": 2,
      "no-restricted-imports": 2,
      "no-restricted-globals": 2,
      "no-restricted-modules": 2,
      "no-script-url": 2,
      "lines-between-class-members": 1,
      "no-multiple-empty-lines": 2,
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error",
            "debug",
            "asset",
            "table",
            "time",
            "timeEnd"
          ]
        }
      ],
      "key-spacing": [
        "error",
        {
          "mode": "strict"
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "shared-node-browser": true,
      "worker": true,
      "amd": true,
      "mocha": true,
      "jasmine": true,
      "jest": true,
      "phantomjs": true,
      "jquery": true,
      "qunit": true,
      "prototypejs": true,
      "shelljs": true,
      "meteor": true,
      "mongo": true,
      "protractor": true,
      "applescript": true,
      "nashorn": true,
      "serviceworker": true,
      "atomtest": true,
      "embertest": true,
      "webextensions": true,
      "es6": true,
      "es2017": true,
      "es2020": true,
      "es2021": true,
      "greasemonkey": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-order": [],
      "font-family-name-quotes": null,
      "function-no-unknown": null,
      "value-keyword-case": null,
      "indentation": 2,
      "color-named": "never",
      "no-eol-whitespace": null,
      "no-descending-specificity": null,
      "custom-property-empty-line-before": null,
      "selector-combinator-space-after": "always",
      "selector-attribute-operator-space-before": "never",
      "declaration-block-trailing-semicolon": "always",
      "declaration-colon-space-before": "never",
      "declaration-colon-space-after": "always",
      "rule-empty-line-before": null,
      "selector-pseudo-element-colon-notation": "double",
      "selector-pseudo-class-parentheses-space-inside": "never",
      "media-feature-parentheses-space-inside": "never",
      "media-feature-colon-space-before": "always",
      "block-closing-brace-empty-line-before": null,
      "at-rule-no-unknown": null,
      "at-rule-name-space-after": null,
      "at-rule-empty-line-before": null,
      "number-leading-zero": null,
      "property-no-unknown": null,
      "max-empty-lines": 2,
      "function-whitespace-after": null,
      "declaration-block-no-duplicate-properties": null,
      "no-duplicate-selectors": null,
      "comment-empty-line-before": null,
      "no-empty-source": null,
      "block-no-empty": null,
      "no-missing-end-of-source-newline": null,
      "color-hex-case": null,
      "media-feature-name-no-unknown": null,
      "selector-type-no-unknown": null,
      "keyframe-declaration-no-important": null,
      "declaration-block-no-shorthand-property-overrides": null,
      "selector-pseudo-class-no-unknown": null,
      "shorthand-property-no-redundant-values": null,
      "function-linear-gradient-no-nonstandard-direction": null,
      "declaration-block-no-redundant-longhand-properties": null,
      "no-unknown-animations": null,
      "color-hex-length": null,
      "selector-class-pattern": null,
      "custom-property-pattern": null,
      "color-function-notation": null,
      "alpha-value-notation": null,
      "string-quotes": "double",
      "value-no-vendor-prefix": null,
      "property-no-vendor-prefix": null,
      "keyframes-name-pattern": null,
      "declaration-colon-newline-after": null,
      "max-line-length": null,
      "selector-list-comma-newline-after": null,
      "length-zero-no-unit": null
    }
  }
}
